    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-router": "^7.4.0",
    "koa-cors": "0.0.16",
    "pug": "^2.0.3",
    "koa-jwt": "^3.6.0"
  },
  "devDependencies": {
    "@types/koa": "^2.0.49",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-jwt": "^3.3.0",
    "@types/koa-router": "^7.0.42",
    "@types/md5": "^2.1.33",
    "@types/mysql": "^2.15.7",
    "@types/node": "^12.7.5",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
