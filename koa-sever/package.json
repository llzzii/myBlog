    "karma-jasmine-html-reporter": "^1.4.2",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-mysql-session": "0.0.2",
    "koa-router": "^7.4.0",
    "koa-session-minimal": "^3.0.4",
    "koa-static": "^5.0.0",
    "koa-static-cache": "^5.1.2",
    "koa-views": "^6.2.0",
    "markdown-it": "^9.0.1",
    "md5": "^2.2.1",
    "mocha": "^6.1.4",
